			<ee:transform doc:name="Transform Message" doc:id="0b4fa02b-6ac3-4e98-bfe3-40059f5aad76" >
				<ee:message >
					<ee:set-payload resource="transform/error.dwl" />
				</ee:message>
			</ee:transform>
